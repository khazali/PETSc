#! /bin/csh -f

sed  "s/[0-9]\.[0-9]*[e]*[-]*[+]*[0-9]*/XXX/g" $1 > tmpA
sed  "s/[0-9]\.[0-9]*[e]*[-]*[+]*[0-9]*/XXX/g" $2 > tmpB
diff tmpA tmpB >& /dev/null
if ($?) then
  diff -y --suppress-common-lines -B $1 $2 > tmpDiff;
  if (`cat tmpDiff | wc -l` < 16) then
    cat tmpDiff
  else
    head -8 tmpDiff
    echo "           MORE DIFFERENCES"
    tail -8 tmpDiff
  endif
  rm -f tmpDiff tmpA tmpB
  exit 1;
else
  rm -f tmpA tmpB
  exit 0;
endif


